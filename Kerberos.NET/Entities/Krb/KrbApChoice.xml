<?xml version="1.0" encoding="utf-8" ?>

<asn:Choice
  xmlns:asn="http://schemas.dot.net/asnxml/201808/"
  name="KrbApChoice"
  namespace="Kerberos.NET.Entities">
  <!--

    This doesn't technically exist as a message, but it's a useful way to let
    The ASN parser figure it out
   
    KrbApChoice ::= CHOICE {
        KrbApReq [APPLICATION 14] KrbApReq,
        KrbApRep [APPLICATION 15] KrbApRep
    }
  -->

  <asn:AsnType name="ApReq" explicitTag="14" tagClass="Application" typeName="KrbApReq" />
  <asn:AsnType name="ApRep" explicitTag="15" tagClass="Application" typeName="KrbApRep" />
</asn:Choice>
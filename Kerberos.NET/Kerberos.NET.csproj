<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>

    <AssemblyVersion>3.0.0.0</AssemblyVersion>
    <FileVersion>3.0.0</FileVersion>

    <VersionPrefix>3.0.0</VersionPrefix>
    <VersionSuffix>alpha</VersionSuffix>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>Kerberos.NET</PackageId>
    <Authors>Steve Syfuhs</Authors>
    <Company>Steve Syfuhs</Company>
    <Product>Kerberos.NET library</Product>
    <Description>A cross-platform, pure managed-code Kerberos Ticket parsing, validation, and authentication library that doesn’t rely on any OS components.</Description>
    <Copyright>Copyright 2019</Copyright>
    <PackageProjectUrl>https://github.com/SteveSyfuhs/Kerberos.NET</PackageProjectUrl>
    <PackageTags>security kerberos</PackageTags>
    <PackageReleaseNotes></PackageReleaseNotes>
    <RepositoryUrl>https://github.com/SteveSyfuhs/Kerberos.NET</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <AsnXml Include="Asn1\Entities\KrbApReq.xml" />
    <AsnXml Include="Asn1\Entities\KrbApRep.xml" />
    <AsnXml Include="Asn1\Entities\KrbAsReq.xml" />
    <AsnXml Include="Asn1\Entities\KrbAuthenticator.xml" />
    <AsnXml Include="Asn1\Entities\KrbAuthenticatorApplication.xml" />
    <AsnXml Include="Asn1\Entities\KrbAuthorizationData.xml" />
    <AsnXml Include="Asn1\Entities\KrbAuthorizationDataSequence.xml" />
    <AsnXml Include="Asn1\Entities\KrbChecksum.xml" />
    <AsnXml Include="Asn1\Entities\KrbCred.xml" />
    <AsnXml Include="Asn1\Entities\KrbCredApplication.xml" />
    <AsnXml Include="Asn1\Entities\KrbCredInfo.xml" />
    <AsnXml Include="Asn1\Entities\KrbEncKrbCredPart.xml" />
    <AsnXml Include="Asn1\Entities\KrbEncKrbCredPartApplication.xml" />
    <AsnXml Include="Asn1\Entities\KrbEncryptedData.xml" />
    <AsnXml Include="Asn1\Entities\KrbEncryptionKey.xml" />
    <AsnXml Include="Asn1\Entities\KrbEncTicketPart.xml" />
    <AsnXml Include="Asn1\Entities\KrbEncTicketPartApplication.xml" />
    <AsnXml Include="Asn1\Entities\KrbError.xml" />
    <AsnXml Include="Asn1\Entities\KrbETypeInfo2.xml" />
    <AsnXml Include="Asn1\Entities\KrbETypeInfo2Entry.xml" />
    <AsnXml Include="Asn1\Entities\KrbETypeInfoEntry.xml" />
    <AsnXml Include="Asn1\Entities\KrbHostAddress.xml" />
    <AsnXml Include="Asn1\Entities\KrbKdcRep.xml" />
    <AsnXml Include="Asn1\Entities\KrbKdcReq.xml" />
    <AsnXml Include="Asn1\Entities\KrbKdcReqBody.xml" />
    <AsnXml Include="Asn1\Entities\KrbMethodData.xml" />
    <AsnXml Include="Asn1\Entities\KrbPaData.xml" />
    <AsnXml Include="Asn1\Entities\KrbPrincipalName.xml" />
    <AsnXml Include="Asn1\Entities\KrbTicket.xml" />
    <AsnXml Include="Asn1\Entities\KrbTicketApplication.xml" />
    <AsnXml Include="Asn1\Entities\KrbTransitedEncoding.xml" />

  </ItemGroup>

  <Import Project="Asn1\AsnXml.targets" />

</Project>